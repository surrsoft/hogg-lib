{
  "version": "0.5.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/surrsoft/hogg-lib",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "hogg-lib",
  "author": "Evgeny Barannikov",
  "module": "dist/hogg-lib-tsdx.esm.js",
  "size-limit": [
    {
      "path": "dist/hogg-lib-tsdx.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hogg-lib-tsdx.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/jest": "^26.0.22",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "size-limit": "^4.10.2",
    "ts-jest": "^26.5.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/isomorphic-fetch": "^0.0.35",
    "airtable": "^0.10.1",
    "isomorphic-fetch": "^3.0.0"
  },
  "keywords": ["database", "airtable"]
}
